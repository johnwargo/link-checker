{
  "name": "@johnwargo/link-checker",
  "version": "0.0.6",
  "description": "A nodejs-based command line utility to validate links in a web application",
  "author": "John M. Wargo",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnwargo/link-checker.git"
  },
